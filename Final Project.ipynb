    "    \n",
    "def marriage_counts():\n",
    "    return cc_default_db['MARRIAGE'].value_counts().apply(f)\n",
    "    \n",
    "def age_counts():\n",
    "    return cc_default_db['AGE'].value_counts().apply(f)\n",
    "\n",
    "def pay_counts():\n",
    "    counts = {}\n",
